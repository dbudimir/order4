    "next": "^9.3.0",
    "next-routes": "^1.4.2",
    "next-seo": "^4.4.0",
    "node-sass": "^4.13.1",
    "npm": "^6.14.2",
    "path-match": "^1.2.4",
    "prop-types": "^15.7.2",
    "react": "^16.13.0",
    "react-dom": "^16.13.0",
    "react-gtm-module": "^2.0.8",
    "react-moment": "^0.9.7",
    "react-share": "^3.0.1",
