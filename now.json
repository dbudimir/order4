{
  "version": 2,
  "name": "order4",
  "builds": [
    {
      "src": "package.json",
      "use": "@now/next@2.1.0"
    }
  ],
  "build": {
    "env": {
      "api_key": "@api_key"
    }
  },
  "routes": [
    {
      "src": "/(.*)/",
      "status": 301,
      "headers": { "Location": "/$1" }
    }
  ]
}
